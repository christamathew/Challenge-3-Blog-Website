<%- include('partials/header'); -%>
<h1>Compose</h1>
<form action="/compose" method="POST">
    <div class="form-group">
    <label>Title</label>
    <input type="text" name="compose_title" class="form-control">
    <label>Post</label>
    <textarea name="compose_post" cols="30" rows="4" class="form-control"></textarea>
    </div>
    <button type="submit" name="submit" class="btn btn-primary">Publish</button>
</form>
<%- include('partials/footer'); -%>